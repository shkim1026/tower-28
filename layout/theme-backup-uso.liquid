{% capture body_class %}{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }} image-hover-light cta-fill-{{settings.cta_button_style}} {% if settings.show_custom_cursor %}custom-cursor{% endif %}{% endcapture %}


<body id="{{ page_title | handle }}" class="{{ body_class }}" >

  {% include 'gtm-container-body' %}
  
  {% section 'popup' %}

  <div class="pageWrap">

  {% section 'header' %}
    <div class="bodyWrap">

      {{ content_for_layout }}

      {% section 'newsletter' %}
      {% section 'footer' %}

    </div> <!-- END bodyWrap -->
  </div> <!-- END pageWrap -->

  <!-- AJAX Mustache Templates -->
  {% include 'ajax-cart' %}
  {% include 'cart-popover' %}

  {{ 'option_selection.js' | shopify_asset_url | script_tag }}
  {{ 'shop.js' | asset_url | script_tag }}
  {% include 'load-spinner' %}
  {% comment %}{% include 'search-autocomplete' %}{% endcomment %}
  <script>Shopify.money_format = '{{ shop.money_format }}';</script>
  
  {% if settings.show_multiple_currencies %}
    {% include 'currencies' %}
  {% endif %}
 {% include 'smile-initializer' %}


  <!-- Afterpay messaging -->
  {% render 'afterpay' %}
  
<script async type="text/javascript" data-api-key="pubkey-s2dSi1X0Iy7RcBL88d11YRICKM0Xpm" id="stamped-script-widget" src="https://cdn2.stamped.io/files/widget.min.js"></script>
{%if template contains "product" %}
  {% assign current_variant = product.selected_or_first_available_variant %}
<script type="application/ld+json">
{
       "@context": "http://schema.org/",
       "@type": "Product",
  	   "url": "{{shop.secure_url | escape}}{{ product.url | escape }}",
  	   "name": "{{ product.title | escape }}",
       "image": "https:{{ product.featured_image.src | img_url: 'master' }}",
       "description": "{{ product.description | escape }}",
       "brand": {
           "name": "{{ product.vendor }}"
       },           
       "sku": "{{ product.variants.first.sku }}",
       "mpn": "{{ product.variants.first.sku }}",
       "offers": {
           "@type": "Offer",
           "priceCurrency": "{{ shop.currency }}",
           "price": "{{ current_variant.price | money_without_currency }}",
           "availability": "http://schema.org/{% if product.available %}InStock{% else %}OutOfStock{% endif %}",
           "seller": {
               "@type": "Organization",
                "name": "{{ shop.name }}"
           },
           "url": "{{shop.secure_url | escape}}{{ product.url | escape }}"
       }
 {% if product.metafields.stamped.reviews_count and product.metafields.stamped.reviews_count != "0" %},"aggregateRating": {
"@type": "AggregateRating",
"ratingValue": {{ product.metafields.stamped.reviews_average | round:2 }},
"reviewCount": {{ product.metafields.stamped.reviews_count }},
"bestRating": 5,
"worstRating": 1
}
{% endif %}
}
</script>

  {% endif %}
</body>


